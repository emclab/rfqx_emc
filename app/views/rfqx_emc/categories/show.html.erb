<div id="main_body">
<h1>分类</h1>
 
<p class="notice"><%= notice %></p>
 
<p>
  <b>名称:</b>
  <%= @category.name %>
</p>
 
<p>
  <b>内容:</b>
  <%= @category.description %>
</p>
 
<p>
  <b>有效?:</b>
  <%= @category.active %>
</p>
<p>
  <b>输入/更改人:</b>
  <%= @category.last_updated_by.name unless @category.last_updated_by_id.nil? %>
</p>

<%= link_to 'Back', SUBURI + "/view_handler?index=0" %>
</div>